{
  "name": "Invio.Extensions.Reflection",
  "title": "Invio Extensions to System.Reflection",
  "version": "0.1.0",

  "authors": [ "Saqib Rokadia <saqib@invioinc.com>" ],

  "buildOptions": {
    "debugType": "portable"
  },

  "tooling": {
    "defaultNamespace": "Invio.Extensions.Reflection"
  },

  "packOptions": {
    "summary": "Extensions to System.Reflection in .Net Core ",
    "tags": [ "reflection", "core" ],
    "owners": [ "Invio, Incorporated" ],
    "licenseUrl": "https://github.com/invio/Invio.Extensions.Reflection/blob/master/LICENSE.txt",
    "projectUrl": "https://github.com/invio/Invio.Extensions.Reflection/",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "git@github.com:invio/Invio.Extensions.Reflection"
    }
  },

  "dependencies": {
    "Microsoft.NETCore.Platforms": "1.0.1",
    "System.Linq": "4.0.0",
    "System.Linq.Expressions": "4.0.0",
    "System.Reflection": "4.0.10",
    "System.Reflection.Extensions": "4.0.0",
    "System.Reflection.TypeExtensions": "4.0.0",
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "dotnet5.4"
      ]
    }
